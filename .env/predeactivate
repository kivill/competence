#!/bin/bash
# This hook is run before this virtualenv is deactivated.

unset PYTHONPATH
unset LOG_PATH
unset LOG_LEVEL

# Database connect parameters
unset DB_NAME
unset DB_HOST
unset DB_PORT
unset DB_USER
unset DB_PASSWORD
unset DB_RECONNECT_ATTEMPTS

# Flask variables
unset FLASK_APP
unset FLASK_ENV
unset JWT_SECRET